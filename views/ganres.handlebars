<div class="main">

	<div class="pageHeader">
        <i class="ion-ios-color-filter-outline"></i>
		<h1>Жанры</h1>
	</div>
	<div class="ganre-list">
		{{#each ganre}}
			<div class="ganre">
				<a href="{{this.url}}">{{this.title}}</a>
				<div>
					{{#each this.subGanres }}<a href="ganre/{{@key}}">{{this}}</a>{{/each}}
				</div>
			</div>

		{{/each}}
	</div>

</div>